{
    "name": "Quick Search",
    "version": "1.0",
    "description": "A one-click solution to automatically google search any text on a website.",
    "commands": {
        "toggle-feature-foo": {
            "suggested_key": {
              "default": "Ctrl+Shift+Y",
              "mac": "Command+Shift+Y"
            },
            "description": "A one-click solution to automatically google search any text on a website."
        }
